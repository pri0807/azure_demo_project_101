{
	"name": "ergastAPI",
	"properties": {
		"parameters": {
			"endpoint": {
				"type": "string"
			},
			"limit": {
				"type": "string"
			},
			"api_url": {
				"type": "string"
			}
		},
		"annotations": [],
		"type": "HttpServer",
		"typeProperties": {
			"url": "@{concat(linkedService().api_url,linkedService().endpoint,'?','limit=',linkedService().limit)}",
			"enableServerCertificateValidation": true,
			"authenticationType": "Anonymous"
		}
	}
}