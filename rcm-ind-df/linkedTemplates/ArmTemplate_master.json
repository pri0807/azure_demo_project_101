{
	"$schema": "http://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
	"contentVersion": "1.0.0.0",
	"parameters": {
		"factoryName": {
			"type": "string",
			"metadata": "Data Factory name",
			"defaultValue": "rcm-ind-df"
		},
		"formula1_delta_lake_accountKey": {
			"type": "secureString",
			"metadata": "Secure string for 'accountKey' of 'formula1_delta_lake'"
		},
		"AzureDatabricks1_properties_typeProperties_existingClusterId": {
			"type": "string",
			"defaultValue": "1221-104515-oxspt2q7"
		},
		"ergastAPI_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "@{concat('http://ergast.com/api/f1/',linkedService().endpoint,'?','limit=',linkedService().limit)}"
		},
		"formula1_delta_lake_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "https://formuladldev.dfs.core.windows.net/"
		},
		"rcminddb_properties_typeProperties_server": {
			"type": "string",
			"defaultValue": "rcm-ind.database.windows.net"
		},
		"rcminddb_properties_typeProperties_database": {
			"type": "string",
			"defaultValue": "@{linkedService().database_name}"
		},
		"rcminddb_properties_typeProperties_userName": {
			"type": "string",
			"defaultValue": "pri060"
		},
		"rcmindkeyvalutdev_properties_typeProperties_baseUrl": {
			"type": "string",
			"defaultValue": "https://rcm-ind-kv-dev.vault.azure.net/"
		},
		"rcmindsto_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "https://devrcmadls001.dfs.core.windows.net/"
		},
		"containerUri": {
			"type": "string",
			"metadata": "URI to storage account containing linked ARM templates"
		},
		"containerSasToken": {
			"type": "string",
			"metadata": "Sas token to storage account containing linked ARM templates"
		}
	},
	"variables": {
		"factoryId": "[concat('Microsoft.DataFactory/factories/', parameters('factoryName'))]"
	},
	"resources": [
		{
			"name": "rcm-ind-df_ArmTemplate_0",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2024-03-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_0.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					},
					"formula1_delta_lake_accountKey": {
						"value": "[parameters('formula1_delta_lake_accountKey')]"
					},
					"ergastAPI_properties_typeProperties_url": {
						"value": "[parameters('ergastAPI_properties_typeProperties_url')]"
					},
					"formula1_delta_lake_properties_typeProperties_url": {
						"value": "[parameters('formula1_delta_lake_properties_typeProperties_url')]"
					},
					"rcmindkeyvalutdev_properties_typeProperties_baseUrl": {
						"value": "[parameters('rcmindkeyvalutdev_properties_typeProperties_baseUrl')]"
					},
					"AzureDatabricks1_properties_typeProperties_existingClusterId": {
						"value": "[parameters('AzureDatabricks1_properties_typeProperties_existingClusterId')]"
					},
					"rcminddb_properties_typeProperties_server": {
						"value": "[parameters('rcminddb_properties_typeProperties_server')]"
					},
					"rcminddb_properties_typeProperties_database": {
						"value": "[parameters('rcminddb_properties_typeProperties_database')]"
					},
					"rcminddb_properties_typeProperties_userName": {
						"value": "[parameters('rcminddb_properties_typeProperties_userName')]"
					},
					"rcmindsto_properties_typeProperties_url": {
						"value": "[parameters('rcmindsto_properties_typeProperties_url')]"
					}
				}
			},
			"dependsOn": []
		}
	]
}